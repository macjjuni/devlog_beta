{
    "extends": [
        "next/core-web-vitals",
        "airbnb",
        "airbnb-typescript",
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": "error"
    }
}
